{
  "address": "transform.bcgov",
  "proxy": "http://test-forwardproxy.nrs.bcgov:23128",
  "os": "linux",
  "os_variant": "rhel7",
  "vault_cd_user_field": "username_lowercase",
  "vault_cd_pass_field": "password",
  "vault_cd_path": "groups/appdelivery/jenkins-isss-cdua",
  "apps": [
    {"id": "cpu", "type": "metric_host_cpu", "context": {"environment": "wfdlv", "inputIntervalSec": 10}},
    {"id": "disk", "type": "metric_host_disk", "context": {"environment": "wfdlv"}},
    {"id": "disk_named", "type": "metric_host_disk_named", "context": {"environment": "wfdlv"}},
    {"id": "memory", "type": "metric_host_memory", "context": {"environment": "wfdlv"}},
    {"id": "network", "type": "metric_host_network", "context": {"environment": "wfdlv"}},
    {
      "id": "wfdlv-wso2",
      "type": "wso2",
      "context": {
        "app": "wso2",
        "component": "api-manager",
        "!logs_path_app": "/sw_ux/wso2/wso2am410/repository/logs/wso2carbon.log",
        "environment": "wfdlv"
      }
    },
    {
      "id": "wfdlv-fmeserver",
      "type": "tomcat",
      "context": {
        "app": "fmeserver",
        "component": "fmeserver",
        "!logs_path_app": "/sw_ux/fme/fmeserver/resources/logs/core/current/fmeserver.log",
        "startAppParserRegex": "/^\\[\\d{4}-\\d{2}-\\d{2}.\\d{2}:\\d{2}:\\d{2}.(?:.\\d{3}?).*$/",
        "contAppParserRegex": "/^[ \\t+a-zA-Z{}#;>\\*\\/].*/",
        "appParserFilterRegex": "(?m)^\\[(?<tomcat_time>\\d{4}-\\d{2}-\\d{2}.\\d{2}:\\d{2}:\\d{2}(.\\d{3})?)\\](\\s+)(?<level>\\S+)(\\s+)(?<message_parsed>.*)",
        "environment": "wfdlv"
      }
    },
    {
      "id": "wfdlv-jcrs",
      "type": "tomcat",
      "context": {
        "app": "jasperserver",
        "component": "jaspersoft-bi",
        "!logs_path_app": "/apps_ux/logs/WFONE/wfjcrs-war/jasperserver_WFJCRS.log",
        "startAppParserRegex": "/^\\[\\d{4}-\\d{2}-\\d{2}.\\d{2}:\\d{2}:\\d{2}.(?:.\\d{3}?).*$/",
        "contAppParserRegex": "/^[ \\t+a-zA-Z{}#;>\\*\\/].*/",
        "appParserFilterRegex": "(?m)^\\[(?<tomcat_time>\\d{4}-\\d{2}-\\d{2}.\\d{2}:\\d{2}:\\d{2}(.\\d{3})?)\\](\\s+)(?<level>\\S+)(\\s+)(?<message_parsed>.*)",
        "environment": "wfdlv"
      }
    }
  ],
  "context": {}
}
