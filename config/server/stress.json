{
  "address": "stress.dmz",
  "proxy": "http://test-forwardproxy.nrs.bcgov:23128",
  "logsProxyDisabled": "true",
  "os": "windows",
  "vault_cd_user_field": "username_domainless",
  "vault_cd_pass_field": "password",
  "vault_cd_path": "groups/appdelivery/oraapp_imborapp",
  "apps": [
    {
      "id": "as-as-api",
      "type": "tomcat",
      "context": {
        "app": "AS",
        "component": "as-as-api",
        "tmplTomcatTestApplicationIndex": "nrm-test-aw-<%=YYYY.MM=%>"
      }
    },
    {
      "id": "gww",
      "type": "winiis",
      "context": {
        "app": "GWW",
        "eventDataset": "iis.access.internal",
        "!logs_path_iis_access": "{{ localLogsPathPrefix | default('E:/Logfiles') }}/{{ app | upper }}/W3SVC10/*.log"
      }
    },
    {
      "id": "www",
      "type": "winiis",
      "context": {
        "app": "Public",
        "eventDataset": "iis.access.internal",
        "!logs_path_iis_access": "{{ localLogsPathPrefix | default('E:/Logfiles') }}/{{ app | upper }}/W3SVC14/*.log"
      }
    }
  ],
  "context": {
    "environment": "test",
    "organization_name": "infrastructure"
  }
}
