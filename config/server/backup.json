        "!logs_path": "{% if localLogsPathPrefix %}{{localLogsPathPrefix}}/wfint{% else %}/sw_ux/httpd02/logs/hot{% endif %}/*-access*.log"
      }
    },
    {
      "id": "wftst-rp",
      "type": "apache_reverse_proxy",
