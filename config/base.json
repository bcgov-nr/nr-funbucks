{
  "context": {
    "appPathJq": "/sw_ux/bin/jq",
    "fluentLogLevel": "error",
    "fluentLogFile": "/apps_ux/agents/main/logs/agent.log",
    "inputDbFile": "/apps_ux/agents/main/db/fluent-bit-logs.db",
    "inputTailMemBufLimit": "5MB",
    "inputTailIgnoreOlder": "7d",
    "output_kinesis": true,
    "output_stdout": true,
    "shimDisableMultilineKey": true,
    "startTomcatParserRegex": "/^\\d{2}-\\w{3}-\\d{4}\\s\\d{2}:\\d{2}:\\d{2}.\\d{3}.*$/",
    "contTomcatParserRegex": "/^[ \\t+].*|Caused by.*/",
    "startAppParserRegex": "/^\\d{4}-\\d{2}-\\d{2}\\s\\d{2}:\\d{2}:\\d{2}(.\\d{3}?).*$/",
    "contAppParserRegex": "/^[ \\t+a-zA-Z{}\\/\\[\\]].*"
  },
  "localContextOverride": {
    "appPathJq": "jq",
    "fluentLogFile": "/tmp/agent.log",
    "localLogsPathPrefix": "/data",
    "inputDbFile": "/tmp/fluent-bit-logs.db",
    "output_kinesis": false,
    "output_local_lambda": true
  },
  "files": [
    { "tmpl": "filters.conf.njk" },
    { "tmpl": "fluent-bit.conf.njk" },
    { "tmpl": "host_metadata.lua" },
    { "tmpl": "outputs.conf.njk" },
    { "tmpl": "parsers.conf.njk" },
    { "tmpl": "timestamp.lua" }
  ]
}
