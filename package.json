{
  "name": "nr-funbucks",
  "version": "1.0.0",
  "description": "Funbucks - A fluentbit configuration generator",
  "author": "Matthew Bystedt",
  "bin": {
    "oclif-hello-world": "./bin/run"
  },
  "homepage": "https://github.com/mbystedt/hello-world",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "repository": "mbystedt/hello-world",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.19.0",
    "@oclif/plugin-help": "^5.1.15",
    "@oclif/plugin-plugins": "^2.1.4",
    "nunjucks": "^3.2.3",
    "semver": "^7.3.8",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.4",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^10.0.0",
    "@types/node": "^16.18.3",
    "@types/nunjucks": "^3.2.1",
    "@types/semver": "^7.3.12",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "chai": "^4",
    "eslint": "^8.25.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jest": "^27.1.2",
    "jest": "^29.2.1",
    "mocha": "^10.1.0",
    "oclif": "^3.2.19",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "oclif": {
    "bin": "nr-funbucks",
    "dirname": "oex",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc.json",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm lint",
    "pack": "",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
